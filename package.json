{
  "name": "karma-ui5",
  "description": "A Karma plugin for UI5",
  "version": "2.0.0",
  "author": "SAP SE (https://www.sap.com)",
  "license": "Apache-2.0",
  "homepage": "https://github.com/SAP/karma-ui5",
  "bugs": "https://github.com/SAP/karma-ui5/issues/",
  "keywords": [
    "ui5",
    "openui5",
    "sapui5",
    "karma-adapter",
    "karma-plugin"
  ],
  "engines": {
    "node": ">= 10.0",
    "npm": ">= 5"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/SAP/karma-ui5.git"
  },
  "main": "lib/index.js",
  "files": [
    "dist",
    "lib"
  ],
  "scripts": {
    "lint": "eslint ./",
    "lint-fix": "eslint ./ --fix",
    "test": "npm run lint && npm run coverage && npm run integration",
    "unit": "ava",
    "unit-watch": "ava --watch",
    "coverage": "nyc npm run unit",
    "integration": "jest -c test/integration/jest.config.js",
    "integration-internet-explorer": "jest -c test/integration/jest.config.js --browsers=IE",
    "dev": "webpack --watch --mode=development",
    "prepare": "webpack",
    "preversion": "npm test",
    "version": "git-chglog --next-tag v$npm_package_version -o CHANGELOG.md && git add CHANGELOG.md",
    "postversion": "git push --follow-tags",
    "release-note": "git-chglog -c .chglog/release-config.yml v$npm_package_version"
  },
  "ava": {
    "files": [
      "test/unit/**/*.js",
      "!test/unit/__helper__/**/*",
      "!test/unit/__stryker__/**/*"
    ]
  },
  "peerDependencies": {
    "karma": ">= 4.3.0"
  },
  "dependencies": {
    "@ui5/fs": "^2.0.0",
    "@ui5/project": "^2.0.3",
    "@ui5/server": "^2.0.0",
    "http-proxy": "^1.18.0",
    "js-yaml": "^3.13.1",
    "router": "^1.3.5"
  },
  "devDependencies": {
    "@babel/core": "^7.9.0",
    "@babel/preset-env": "^7.9.0",
    "@openui5/sap.ui.core": "^1.76.0",
    "@stryker-mutator/core": "^3.1.0",
    "@stryker-mutator/javascript-mutator": "^3.1.0",
    "ava": "^3.6.0",
    "babel-loader": "^8.1.0",
    "core-js": "^3.6.4",
    "create-cert": "^1.0.6",
    "eslint": "^6.8.0",
    "eslint-config-google": "^0.14.0",
    "eslint-plugin-jest": "^23.8.2",
    "eslint-plugin-jsdoc": "^22.1.0",
    "execa": "^3.4.0",
    "istanbul-lib-coverage": "^3.0.0",
    "jest": "^25.2.7",
    "jest-extended": "^0.11.5",
    "karma": "^4.4.1",
    "karma-chrome-launcher": "^3.1.0",
    "karma-coverage": "^2.0.1",
    "karma-edge-launcher": "^0.4.2",
    "karma-ie-launcher": "^1.0.0",
    "karma-qunit": "^4.0.0",
    "license-webpack-plugin": "^2.1.4",
    "mock-require": "^3.0.3",
    "nyc": "^15.0.1",
    "puppeteer": "^2.1.1",
    "qunit": "^2.9.3",
    "rimraf": "^3.0.2",
    "sinon": "^9.0.1",
    "syswide-cas": "^5.3.0",
    "typescript": "^3.8.3",
    "webpack": "^4.42.1",
    "webpack-cli": "^3.3.11"
  }
}
